{
  "$schema": "../source_pack/03_taxonomy/schemas/changelog.schema.json",
  "standard_id": "AIMO-STD",
  "entries": [
    {
      "version": "0.1.0",
      "date": "2026-01-31",
      "change_type": "minor",
      "backward_compatible": true,
      "summary": "Initial release of AIMO Standard with 8-dimension code system and Evidence Pack templates.",
      "details": [
        {
          "type": "added",
          "scope": "FS",
          "description": "Functional Scope dimension with 6 codes (FS-001 to FS-006)"
        },
        {
          "type": "added",
          "scope": "UC",
          "description": "Use Case Class dimension with 10 codes (UC-001 to UC-010)"
        },
        {
          "type": "added",
          "scope": "DT",
          "description": "Data Type dimension with 8 codes (DT-001 to DT-008)"
        },
        {
          "type": "added",
          "scope": "CH",
          "description": "Channel dimension with 6 codes (CH-001 to CH-006)"
        },
        {
          "type": "added",
          "scope": "IM",
          "description": "Integration Mode dimension with 5 codes (IM-001 to IM-005)"
        },
        {
          "type": "added",
          "scope": "RS",
          "description": "Risk Surface dimension with 5 codes (RS-001 to RS-005)"
        },
        {
          "type": "added",
          "scope": "OB",
          "description": "Outcome / Benefit dimension with 5 codes (OB-001 to OB-005)"
        },
        {
          "type": "added",
          "scope": "EV",
          "description": "Log/Event Type dimension (LG) with 7 codes (LG-001 to LG-007); EV reserved for Evidence artifact IDs only"
        },
        {
          "type": "added",
          "scope": "schema",
          "description": "taxonomy_pack.schema.json for taxonomy pack definition"
        },
        {
          "type": "added",
          "scope": "schema",
          "description": "changelog.schema.json for changelog entries"
        },
        {
          "type": "added",
          "scope": "schema",
          "description": "evidence_pack_manifest.schema.json for Evidence Pack manifests"
        },
        {
          "type": "added",
          "scope": "template",
          "description": "Evidence Pack templates EV-01 through EV-07"
        }
      ]
    }
  ]
}
